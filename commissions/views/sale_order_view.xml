<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="sale_order_commission_form_view">
            <field name="name">sale.order.commission.form.view</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <field name="date_order" position="after">
                    <field name="dont_calculate"/>
                    <field name="net_margin"/>
                    <field name="net_margin_por"/>
                    <field name="gross_margin"/>
                </field>
            </field>
        </record>
    </data>
</odoo>